        <client_script><![CDATA[function($rootScope, $scope, spModal, spUtil) {
    var c = this;

    $scope.data.currentAnswer = "";

    $scope.$on('record.updated', function(name, data) {
        console.log('Ferguson QandA Formatter received broadcast event of record.updated');
        spUtil.update($scope);
    });

    var modalInstance;

    c.openRelatedRecord = function(widgetId, widgetInput, title) {
        modalInstance = spModal.open({
            title: title,
            buttons: [],
            widget: widgetId,
            widgetInput: widgetInput,
            size: 'lg'
        });
    };
	
	  c.emitAnswers = function(id, val) {
				 $scope.$emit('pa_response_answers_scope_emit', $scope.data.responses);			
		};

