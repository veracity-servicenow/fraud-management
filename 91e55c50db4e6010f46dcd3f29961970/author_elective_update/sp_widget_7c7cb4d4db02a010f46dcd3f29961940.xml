				 $scope.$emit('pa_response_answers_scope_emit', $scope.data.responses);

			spUtil.update($scope).then(function(data) {
            console.log('Ferguson QandA Formatter spUtil update');
            //spUtil.addInfoMessage("testing info message from controller");
        });

        $scope.server.get({
				    answers: $scope.c.data.responses,
            data: $scope.c.data
        }).then(function(response) {
            console.log("server get response using $scope.server.get after broadcast " + response.data.responses[0]._fields.answer.value);
        });
        //if (modalInstance) modalInstance.close();
    });
*/
	

}]]></client_script>
